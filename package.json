{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run build:production && ./node_modules/.bin/forever ./bin/www",
    "start:development": "./node_modules/.bin/nodemon ./bin/www",
    "build:production": "cd ./client && webpack -p --progress && cd .. ",
    "start:development_conc": "concurrently --kill-others \" cd client && npm start \" \" cd node_modules/.bin && nodemon ../.././bin/www \" "
  },
  "author": "Arpit Goyal <arpit.go4@gmail.com> (http://github.com/arpitgo4)",
  "dependencies": {
    "body-parser": "~1.16.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.0",
    "ejs": "^2.5.6",
    "express": "~4.14.1",
    "forever": "^0.15.3",
    "mongoose": "^4.9.0",
    "morgan": "~1.7.0",
    "serve-favicon": "~2.3.2"
  },
  "devDependencies": {
    "concurrently": "^3.4.0",
    "nodemon": "^1.11.0"
  }
}
